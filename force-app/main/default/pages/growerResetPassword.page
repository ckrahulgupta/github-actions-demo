<apex:page showHeader="false" 
    sidebar="false" 
    standardStylesheets="false" 
    applyHtmlTag="false" 
    applyBodyTag="false" 
    docType="html-5.0"
    controller="clcommon.PortalActions"
    extensions="PortalDealerPersonalizeControllerExt"
>
    <apex:include pageName="clcommon__ResolveNamespace" />
    <apex:variable var="coopType" value="{!CoopTypeFromLoggedInUser}" />
    <html>
        <head>
            <meta 
                name="viewport" 
                content="width=device-width, initial-scale=1, maximum-scale=1,user-scalable=0"
            />

            <!-- for dev -->
            <!-- <apex:stylesheet value="http://localhost:3000/dist/css/style.css" /> -->
            <!-- <apex:includeScript loadOnReady="true" 
                            value="http://localhost:3000/dist/js/growerPartnerProgramBrandingJS.js"/> -->

            <!-- for prod -->
            <apex:stylesheet value="{!URLFOR($Resource.growerPortalCSS)}" />
			<apex:includeScript loadOnReady="true" 
                        value="{!URLFOR($Resource.growerPartnerProgramBrandingJS)}"/>

        </head>
        <body>
            <!-- navigation structure name include-->
            <clcommon:IncludePortal2 navName="portal_grower_reset_password" theme="mui"></clcommon:IncludePortal2>
        </body>
    </html>
</apex:page>